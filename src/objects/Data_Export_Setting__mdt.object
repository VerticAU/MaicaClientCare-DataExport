<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Configure settings for Data Export processing.</description>
    <label>Data Export Setting</label>
    <pluralLabel>Data Export Settings</pluralLabel>
    <visibility>Public</visibility>
    <sharingModel>ReadWrite</sharingModel>

    <fields>
        <fullName>Chunk_Limit__c</fullName>
        <label>Chunk Limit</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <precision>18</precision>
        <scale>0</scale>
    </fields>

    <fields>
        <fullName>Format__c</fullName>
        <label>Format</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>CSV</fullName>
                    <default>true</default>
                    <label>CSV</label>
                </value>
                <value>
                    <fullName>XML</fullName>
                    <default>false</default>
                    <label>XML</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>

    <fields>
        <fullName>Is_Active__c</fullName>
        <label>Is Active</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
        <defaultValue>false</defaultValue>
    </fields>
</CustomObject>
